{
  "name": "sitessu",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:wrangler": "wrangler pages dev --local --d1=DB",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "db:init": "wrangler d1 execute francekhalil-db --local --file=./db/schema.sql",
    "db:seed": "wrangler d1 execute francekhalil-db --local --file=./db/seed.sql",
    "db:migrate": "wrangler d1 migrations apply francekhalil-db --local",
    "db:remote:init": "wrangler d1 execute francekhalil-db --remote --file=./db/schema.sql",
    "db:remote:seed": "wrangler d1 execute francekhalil-db --remote --file=./db/seed.sql"
  },
  "dependencies": {
    "dompurify": "^3.3.1",
    "lucide-react": "^0.560.0",
    "marked": "^17.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "vite": "^7.2.4",
    "wrangler": "^3.115.0"
  }
}